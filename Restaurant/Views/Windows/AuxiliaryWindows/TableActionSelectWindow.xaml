<Window x:Class="Restaurant.Views.Windows.AuxiliaryWindows.TableActionSelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Restaurant.Views.Windows.AuxiliaryWindows"
        mc:Ignorable="d"
        Title="TableActionSelectWindow"
        Height="50"
        Width="303"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        ShowInTaskbar="False" BorderThickness="1">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="110" />
            <ColumnDefinition Width="190" />
        </Grid.ColumnDefinitions>
        <Button x:Name="BackBtn"
                Content="Отмена"
                Click="BackBtn_Click"
                Grid.Column="0"
                Width="100"/>
        <Button x:Name="DereserveTableBtn"
                Content=""
                Click="DereserveTableBtn_Click"
                Grid.Column="1"
                Width="180"/>
    </Grid>
</Window>
